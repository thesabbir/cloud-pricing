{
  "name": "cloud-pricing",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:wrangler": "wrangler dev --x-remote-bindings",
    "dev:remote": "wrangler dev --remote",
    "build": "vite build",
    "preview": "$npm_execpath run build && vite preview",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@scalar/hono-api-reference": "^0.9.16",
    "hono": "^4.9.6",
    "hono-openapi": "^1.0.0",
    "openai": "^5.19.1",
    "zod": "^4.1.5",
    "zod-openapi": "^5.4.0"
  },
  "devDependencies": {
    "@cloudflare/playwright": "^0.0.11",
    "@cloudflare/vite-plugin": "^1.2.3",
    "@types/node": "^24.3.1",
    "vite": "^6.3.5",
    "vite-ssr-components": "^0.5.1",
    "wrangler": "^4.17.0"
  }
}
